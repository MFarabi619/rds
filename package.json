{
  "name": "@carletonuniversity/rds",
  "private": false,
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "concurrently 'npm run dev:css' 'tsc && vite build --watch'",
    "dev:css": "tailwindcss -w -i ./src/styles/index.css -o ./public/index.css",
    "build": "tsc && npm run format && npm run lint && vite build && npm run build:css && npm run size",
    "build:css": "tailwindcss -m -i ./src/styles/index.css -o ./dist/index.css",
    "storybook": "concurrently 'npm run storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "tailwindcss -w -i ./src/styles/index.css -o ./.storybook/index.css",
    "build-storybook": "concurrently 'npm run build-storybook:css' 'storybook build'",
    "build-storybook:css": "tailwindcss -m -i ./src/styles/index.css -o ./.storybook/index.css",
    "size": "size-limit",
    "format": "prettier -w src",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ; yarn install --frozen-lockfile"
  },
  "resolutions": {
    "prettier": "2.8.1"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "rds-tailwind-theme": "0.5.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "9.5.3",
    "tailwindcss": "3.3.6"
  },
  "peerDependencies": {
    "@react-google-maps/api": "2.19.2",
    "formik": "2.4.5",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.13.0",
    "react-quill": "2.0.0",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@react-google-maps/api": "2.19.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@size-limit/esbuild": "11.0.1",
    "@size-limit/esbuild-why": "11.0.1",
    "@size-limit/file": "11.0.1",
    "@storybook/addon-a11y": "7.6.4",
    "@storybook/addon-essentials": "7.6.4",
    "@storybook/addon-interactions": "7.6.4",
    "@storybook/addon-links": "7.6.4",
    "@storybook/blocks": "7.6.4",
    "@storybook/react": "7.6.4",
    "@storybook/react-vite": "7.6.4",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@types/react-places-autocomplete": "7.2.14",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@vitejs/plugin-react": "4.2.1",
    "@whitespace/storybook-addon-html": "5.1.6",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "cypress": "13.6.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "0.6.15",
    "formik": "2.4.5",
    "husky": "8.0.3",
    "postcss": "8.4.32",
    "postcss-import": "15.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.9",
    "prop-types": "15.8.1",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.13.0",
    "react-quill": "2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "size-limit": "11.0.1",
    "storybook": "7.6.4",
    "typescript": "5.3.3",
    "vite": "5.0.6",
    "vite-plugin-dts": "3.6.4",
    "vite-tsconfig-paths": "4.2.1",
    "yup": "1.3.2"
  }
}
