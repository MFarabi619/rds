{
  "name": "@carletonuniversity/rds",
  "private": false,
  "version": "0.0.41",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js"
    }
  },
  "scripts": {
    "size": "size-limit",
    "format": "prettier -w src",
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "concurrently 'npm run dev:css' 'tsc && vite build --watch'",
    "dev:css": "tailwindcss -w -i ./src/styles/index.css -o ./public/index.css",
    "build": "tsc && npm run format && npm run lint && vite build && npm run build:css && npm run size",
    "build:css": "tailwindcss -m -i ./src/styles/index.css -o ./dist/index.css",
    "cypress": "concurrently 'npm run cypress:css' 'cypress open'",
    "cypress:ci": "concurrently 'npm run cypress:css' 'cypress run --component'",
    "cypress:css": "tailwindcss -w -i ./src/styles/index.css -o ./cypress/support/index.css",
    "storybook": "concurrently 'npm run storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "tailwindcss -w -i ./src/styles/index.css -o ./.storybook/index.css",
    "build-storybook": "concurrently 'npm run build-storybook:css' 'storybook build'",
    "build-storybook:css": "tailwindcss -m -i ./src/styles/index.css -o ./.storybook/index.css",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ; npm ci ; npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@headlessui/react": "1.7.12",
    "@heroicons/react": "^2.0.16",
    "@react-google-maps/api": "^2.18.1",
    "@size-limit/esbuild": "^8.2.4",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-a11y": "^7.0.7",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-mdx-gfm": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/react-places-autocomplete": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@whitespace/storybook-addon-html": "^5.1.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cypress": "^12.10.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "formik": "^2.2.9",
    "husky": "^8.0.3",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prop-types": "^15.8.1",
    "rds-tailwind-theme": "^0.3.0",
    "react-places-autocomplete": "^7.3.0",
    "react-player": "^2.12.0",
    "react-quill": "^2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0",
    "yup": "^1.1.1"
  },
  "peerDependencies": {
    "@headlessui/react": "1.7.12",
    "@heroicons/react": "^2.0.16",
    "@react-google-maps/api": "^2.18.1",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.12.0",
    "react-quill": "^2.0.0",
    "tailwindcss": "^3.3.1",
    "yup": "^1.1.1"
  }
}
