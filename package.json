{
  "name": "@carletonuniversity/rds",
  "private": false,
  "version": "0.1.3",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js"
    }
  },
  "scripts": {
    "size": "size-limit",
    "format": "prettier -w src",
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "concurrently 'npm run dev:css' 'tsc && vite build --watch'",
    "dev:css": "tailwindcss -w -i ./src/styles/index.css -o ./public/index.css",
    "build": "tsc && npm run format && npm run lint && vite build && npm run build:css && npm run size",
    "build:css": "tailwindcss -m -i ./src/styles/index.css -o ./dist/index.css",
    "cypress": "concurrently 'npm run cypress:css' 'cypress open'",
    "cypress:ci": "concurrently 'npm run cypress:css' 'cypress run --component'",
    "cypress:css": "tailwindcss -w -i ./src/styles/index.css -o ./cypress/support/index.css",
    "storybook": "concurrently 'npm run storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "tailwindcss -w -i ./src/styles/index.css -o ./.storybook/index.css",
    "build-storybook": "concurrently 'npm run build-storybook:css' 'storybook build'",
    "build-storybook:css": "tailwindcss -m -i ./src/styles/index.css -o ./.storybook/index.css",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ; npm ci ; npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@react-google-maps/api": "2.19.2",
    "@size-limit/esbuild": "8.2.6",
    "@size-limit/esbuild-why": "8.2.6",
    "@size-limit/file": "8.2.6",
    "@storybook/addon-a11y": "7.3.2",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-mdx-gfm": "7.3.2",
    "@storybook/blocks": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-vite": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-places-autocomplete": "7.2.9",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "@vitejs/plugin-react": "4.0.4",
    "@whitespace/storybook-addon-html": "5.1.6",
    "autoprefixer": "10.4.15",
    "concurrently": "8.2.0",
    "cypress": "12.17.4",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "0.6.13",
    "formik": "2.4.3",
    "husky": "8.0.3",
    "postcss": "8.4.28",
    "postcss-import": "15.1.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prop-types": "15.8.1",
    "rds-tailwind-theme": "0.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.12.0",
    "react-quill": "2.0.0",
    "react-syntax-highlighter": "15.5.0",
    "size-limit": "8.2.6",
    "storybook": "7.3.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-dts": "3.5.2",
    "vite-tsconfig-paths": "4.2.0",
    "yup": "1.2.0"
  },
  "peerDependencies": {
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@react-google-maps/api": "2.19.2",
    "formik": "2.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-player": "2.12.0",
    "react-quill": "2.0.0",
    "tailwindcss": "3.3.3",
    "yup": "1.2.0"
  },
  "dependencies": {
    "react-intersection-observer": "^9.5.2"
  }
}
